{
  "name": "react1",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npx ts-node --esm server.ts\"",
    "build": "concurrently \"npm run server\" \"rm -rf dist && npm run build:client && npm run build:server\"",
    "build:client": "vite build --outDir dist/client",
    "server": "json-server -w server/db.json -p 5123",
    "build:server": "vite build --ssr src/entry-server.tsx --outDir dist/server",
    "serve": "NODE_ENV=production node server",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint --ext .tsx src --fix",
    "test": "vitest --coverage"
  },
  "dependencies": {
    "@remix-run/node": "^1.15.0",
