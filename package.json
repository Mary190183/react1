{
  "name": "react1",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npx ts-node --esm server.ts\"",
    "build": "concurrently \"npm run server\" \"rm -rf dist && npm run build:client && npm run build:server\"",
    "build:client": "vite build --outDir dist/client",
    "server": "json-server -w server/db.json -p 5173 --no-cors",
    "build:server": "vite build --ssr src/entry-server.tsx --outDir dist/server",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "cypress": "cypress open",
    "cover": "cypress run --env coverage=true",
    "report": "npx nyc report --reporter=text-summary",
    "test": "vitest --coverage",
    "type": "tsc"
  },
  "devDependencies": {
    "@cypress/code-coverage": "^3.10.4",
    "@nabla/vite-plugin-eslint": "^1.5.0",
    "@redux-devtools/core": "^3.13.1",
    "@reduxjs/toolkit": "^1.9.5",
    "@remix-run/node": "^1.15.0",
    "@svgx/vite-plugin-react": "^1.0.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.1",
    "@types/mocha": "^10.0.1",
    "@types/node": "^18.16.3",
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "@typescript-eslint/parser": "^5.59.1",
    "@vitejs/plugin-react": "^4.0.0",
    "@vitest/coverage-c8": "^0.30.1",
    "@vitest/coverage-istanbul": "^0.30.1",
    "compression": "^1.7.4",
    "concurrently": "^8.0.1",
    "cookie-parser": "^1.4.6",
    "cross-env": "^7.0.3",
    "cypress": "^12.11.0",
    "eslint": "^8.39.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "express": "^4.18.2",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "json-server": "^0.17.3",
    "lint": "^0.8.19",
    "lint-staged": "^13.2.2",
    "nyc": "^15.1.0",
    "prettier": "^2.8.8",
    "react-hook-form": "^7.43.9",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.11.0",
    "serve-static": "^1.15.0",
    "typescript": "^5.0.4",
    "vite": "^4.3.3",
    "vite-plugin-checker": "^0.5.6",
    "vite-plugin-istanbul": "^4.0.1",
    "vite-plugin-react": "^4.0.1",
    "vite-plugin-svgr": "^2.4.0",
    "vitest": "^0.30.1"
  },
  "nyc": {
    "reporter": [
      "html"
    ]
  }
}
